<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/navigation_main">

    <fragment
        android:id="@+id/navigation_main"
        android:name="com.example.kosmos.chooseyoursign.view.ChooseYourSignFragment"
        android:label="Choose Your Sign"
        tools:layout="@layout/fragment_choose_your_sign">
        <action
            android:id="@+id/action_navigation_choose_your_sign_to_home"
            app:destination="@id/navigation_home"/>
    </fragment>

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.kosmos.home.HomeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_home">
    </fragment>
</navigation>